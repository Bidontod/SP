//Program 1://

Program _prg;

Start Var Int_4 _AAA, _BBB, _XXX, _YYY;

Scan(_AAA);
Scan(_BBB);

Print(_AAA ++ _BBB);
Print(_AAA -- _BBB);
Print(_AAA** _BBB);
Print(_AAA Div _BBB);
Print(_AAA Mod _BBB);

(_AAA -- _BBB) ** 10 ++ (_AAA ++ _BBB) Div 10 = > _XXX;
_XXX + _XXX Mod 10 = > _YYY;

Print(_XXX);
Print(_YYY);

Finish;

